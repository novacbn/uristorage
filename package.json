{
    "name": "uristorage",
    "version": "0.0.2",
    "description": "Provides abstractions for working with different backends as a simplistic Object Storage",
    "author": "novacbn",
    "main": "lib/index.js",
    "module": "dist/esm/index.js",
    "global": "URIStorage",
    "typings": "types/index.d.ts",
    "keywords": [
        "browser",
        "library",
        "node",
        "storage"
    ],
    "files": [
        "dist",
        "lib",
        "types"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/novacbn/uristorage"
    },
    "scripts": {
        "build:lib": "node ./scripts/build.js",
        "build:mime-types": "node ./scripts/build_mime_types.js",
        "build:types": "tsc --emitDeclarationOnly",
        "build:typedoc": "typedoc --out ./dist/docs --exclude '**/index.ts' --stripInternal --target es6 --theme minimal --mode modules src",
        "test:http": "sirv ./ --port 8080 --dev",
        "test:types": "tsc --noEmit",
        "prebuild:clean": "rimraf ./dist;rimraf ./lib"
    },
    "dependencies": {
        "base85": "git+https://github.com/novacbn/base85.git",
        "dexie": "^3.0.2",
        "dexie-observable": "^3.0.0-beta.10",
        "glob-to-regexp": "^0.4.1",
        "lzutf8": "^0.5.6",
        "path-browserify": "^1.0.1"
    },
    "devDependencies": {
        "@types/glob-to-regexp": "^0.4.0",
        "esbuild": "^0.7.22",
        "glob": "^7.1.6",
        "mime-types": "^2.1.27",
        "prettier": "^2.0.5",
        "rimraf": "^3.0.2",
        "sirv-cli": "^1.0.6",
        "typedoc": "^0.19.2",
        "typescript": "^4.0.3"
    }
}
